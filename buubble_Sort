def bubble_sort(arr):
    n = len(arr)
    for i in range(n-1):
        swap = False
        for j in range(n-i-1):
            if arr[j]>arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
                swap = True
        if swap = False:
            return
    return arr

x = [6,2,9,0,13,8,8]

print(bubble_sort(x))
